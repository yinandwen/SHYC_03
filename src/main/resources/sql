/*Table structure for table `project` */

DROP TABLE IF EXISTS `project`;

CREATE TABLE `project` (
  `ORDER_NAME` varchar(128) default NULL COMMENT '订单-项目名称',
  `PRJ_ID` int(11) NOT NULL auto_increment COMMENT '项目ID',
  `PRJ_CODE` varchar(32) default NULL COMMENT '项目号',
  `PRJ_DETAILS` varchar(512) default NULL COMMENT '项目内容-备注',
  `PRJ_MANAGER_ID` varchar(32) default NULL COMMENT '项目经理',
  `PRJ_PlanStartDay` date default NULL COMMENT '计划启动日期',
  `PRJ_PlanDeliveryDay` date default NULL COMMENT '计划交付日期',
  `PRJ_START_DATE` date default NULL COMMENT '启动日期',
  `PRJ_ReadyGo` date default NULL COMMENT '数据接收日期',
  `PRJ_DELIVERY_DATE` date default NULL COMMENT '交付日期',
  `PRJ_SITUATION` varchar(32) default NULL COMMENT '项目状态',
  `PRJ_ALL_TASKS` int(11) default NULL COMMENT '任务数量',
  `PRJ_FINISH_TASKS` int(11) default NULL COMMENT '任务完成数量',
  `PRJ_FINISH_RATE` double(5,1) default NULL COMMENT '项目任务完成率',
  `PRJ_ApprovedTime` datetime default NULL COMMENT '批准时间',
  `PRJ_DrawingCount` int(255) default NULL COMMENT '图纸总数',
  `PRJ_DrawingCount_Released` int(11) default NULL COMMENT '已发现图纸总数',
  `PRJ_DrawingCount_Machined` int(11) default NULL COMMENT '已加工图纸总数',
