/*Table structure for table `desc` */

DROP TABLE IF EXISTS `desc`;

CREATE TABLE `desc` (
  `id` int(11) NOT NULL auto_increment COMMENT 'id',
  `projectId` int(11) NOT NULL COMMENT '项目id',
  `problemType` varchar(32) default NULL COMMENT '问题类型',
  `problemDesc` varchar(32) NOT NULL COMMENT '问题描述',
  `projectManager` varchar(32) NOT NULL COMMENT '责任人',
  `startDate` date NOT NULL COMMENT '项目开始日期',
  `image` varchar(255) default NULL COMMENT '上传图片',
  PRIMARY KEY  (`id`),
  KEY `id` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=123456790 DEFAULT CHARSET=utf8;

