/*Table structure for table `employees` */

DROP TABLE IF EXISTS `employees`;

CREATE TABLE `employees` (
  `US_ROLE_ID` int(11) default NULL COMMENT '担任职位',
  `US_DEPT_ID` int(32) default NULL COMMENT '所属部门',
  `US_ID` int(11) NOT NULL COMMENT '用户ID',
  `US_CODE` varchar(32) NOT NULL COMMENT '工号',
  `US_PASSWORD` varchar(32) default NULL COMMENT '密码',
  `US_SALT` varchar(32) default NULL COMMENT '密码盐值',
  `US_AVATAR` varchar(32) default NULL COMMENT '员工头像',
  `US_NAME` varchar(32) default NULL COMMENT '员工姓名',
  `US_ENG_NAME` varchar(32) default NULL COMMENT '英文名',
  `US_IDCARD_NO` varchar(32) default NULL COMMENT '身份证号',
  `US_MOBILE_PHONE` varchar(32) default NULL COMMENT '手机号',
  `US_PHONE` varchar(32) default NULL COMMENT '公司短号',
  `US_EMAIL` varchar(32) default NULL COMMENT '邮箱',
  `US_SALARY` decimal(32,0) default NULL COMMENT '薪资',
  `US_SALARY_CARD_NO` varchar(32) default NULL COMMENT '薪资卡',
  `US_GENDER` varchar(5) default NULL COMMENT '性别',
  `US_STATUS` varchar(32) default NULL COMMENT '状态',
  `US_EDUCATION` varchar(32) default NULL COMMENT '学历',
  `US_BIRTHDAY` date default NULL COMMENT '生日',
  `US_LOADTICE` int(11) default NULL COMMENT '登录次数',
  `US_ONLINE_TIME` time default NULL COMMENT '登录时长',
  `US_LOAD_TIME` datetime default NULL COMMENT '登入时间',
  `US_LEAVE_TIME` datetime default NULL COMMENT '登出时间',
  `US_Apprevision` varchar(32) default NULL COMMENT '软件版本',
  `US_REVISION` int(11) default NULL COMMENT '乐观锁',
  `US_CREATED_BY` varchar(32) default NULL COMMENT '创建人',
  `US_CREATED_TIME` datetime default NULL COMMENT '创建时间',
  `US_UPDATED_BY` varchar(32) default NULL COMMENT '更新人',
  `US_UPDATED_TIME` datetime default NULL COMMENT '更新时间',
  PRIMARY KEY  (`US_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `employees` */

LOCK TABLES `employees` WRITE;

UNLOCK TABLES;

/*Table structure for table `orders` */

DROP TABLE IF EXISTS `orders`;

CREATE TABLE `orders` (
  `PO_ID` int(11) NOT NULL auto_increment COMMENT '订单',
  `PO_CUSTOMER` varchar(32) NOT NULL COMMENT '客户简称',
  `PO_FullNameCustomer` varchar(255) default NULL COMMENT '客户全名',
  `PO_NAME` varchar(128) NOT NULL COMMENT '订单名/外部项目名',
  `PO_PRJCODE` varchar(32) default NULL COMMENT '项目号',
  `PO_PROTYPE` varchar(32) default NULL COMMENT '产品类型',
  `PO_Details` varchar(255) default NULL COMMENT '制作内容',
  `PO_DeliveyA` varchar(128) default NULL COMMENT '预计交期--交期A',
  `PO_ShipTo` varchar(255) default NULL COMMENT '交货地址',
  `PO_SALESMAN` varchar(32) NOT NULL COMMENT '销售员',
  `PO_CONTRACT_NO` varchar(32) default NULL COMMENT '合同号',
  `PO_During` varchar(0) default NULL COMMENT '账期',
  `PO_CONTRAC_DATE` date default NULL COMMENT '签订日期',
  `PO_FILLING_DATE` varchar(32) default NULL COMMENT '归档日期',
  `PO_SUMS` decimal(32,0) NOT NULL COMMENT '合同金额',
  `PO_ARRIVED_SUMS` decimal(32,0) NOT NULL COMMENT '回款金额',
  `PO_PAYMENT_TIMES` int(11) NOT NULL COMMENT '应收期数',
  `PO_ARRIVED_TIMES` int(11) NOT NULL COMMENT '已付期数',
  `PO_AREA` varchar(32) default NULL COMMENT '销售市场',
  `PO_AmountOutTax` decimal(32,0) NOT NULL COMMENT '净值金额',
  `PO_HowToPay` varbinary(128) default NULL COMMENT '结算方式',
  `PO_ClearningForENC` varchar(255) default NULL COMMENT '设变结算',
  `PO_SeverItem` varchar(128) default NULL COMMENT '服务要求',
  `PO_Digou` varchar(255) NOT NULL COMMENT '代购设备',
  `PO_NOTE` varchar(512) NOT NULL COMMENT '订单备注',
  `PO_ApprovedTime` datetime default NULL COMMENT '批准时间',
  `PO_REVISION` int(11) default NULL COMMENT '乐观锁',
  `PO_CREATED_BY` varchar(32) default NULL COMMENT '创建人',
  `PO_CREATED_TIME` datetime default NULL COMMENT '创建时间',
  `PO_UPDATED_BY` varchar(32) default NULL COMMENT '更新人',
  `PO_UPDATED_TIME` datetime default NULL COMMENT '更新时间',
  PRIMARY KEY  (`PO_ID`)
) ENGINE=InnoDB AUTO_INCREMENT=19 DEFAULT CHARSET=utf8;

/*Data for the table `orders` */

LOCK TABLES `orders` WRITE;

