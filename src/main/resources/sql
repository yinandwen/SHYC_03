/*Table structure for table `problemdesc` */

DROP TABLE IF EXISTS `problemdesc`;

CREATE TABLE `problemdesc` (
  `projectId` varchar(60) NOT NULL COMMENT '项目id',
  `submitId` varchar(60) NOT NULL COMMENT '检具id',
  `submitName` varchar(60) NOT NULL COMMENT '检具名称',
  `problemType` varchar(60) NOT NULL COMMENT '问题类型',
  `problemDesc` varchar(255) NOT NULL COMMENT '问题点',
  `problemWhy` varchar(255) default NULL COMMENT '为什么发生这个问题',
  `startTime` date NOT NULL COMMENT '什么时候发生的',
  `projectManager` varchar(32) NOT NULL COMMENT '项目负责人',
  `imageDesc` varchar(255) default NULL COMMENT '图片进行补充描述',
  `problemStatus` int(11) NOT NULL default '0' COMMENT '问题状态',
  `create_time` datetime default NULL COMMENT '创建时间',
  `update_time` datetime default NULL COMMENT '更新时间',
  PRIMARY KEY  (`projectId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

